        url('<%= asset_path 'RobotoSlab-Light-webfont.woff' %>') format('woff'),
        url('<%= asset_path 'RobotoSlab-Light-webfont.ttf' %>') format('truetype'),
        url('<%= asset_path 'RobotoSlab-Light-webfont.svg#roboto_slablight' %>') format('svg');
    font-weight: normal;
    font-style: normal;

}
@font-face {
    font-family: 'roboto_slabregular';
    src:url('<%= asset_path 'RobotoSlab-Regular-webfont.eot' %>');
    src:url('<%= asset_path 'RobotoSlab-Regular-webfont.eot?#iefix' %>') format('embedded-opentype'),
        url('<%= asset_path 'RobotoSlab-Regular-webfont.woff' %>') format('woff'),
        url('<%= asset_path 'RobotoSlab-Regular-webfont.ttf' %>') format('truetype'),
        url('<%= asset_path 'RobotoSlab-Regular-webfont.svg#roboto_slabregular' %>') format('svg');
    font-weight: normal;
    font-style: normal;

}

@font-face {
    font-family: 'roboto_slabbold';
    src: url('<%= asset_path 'RobotoSlab-Bold-webfont.eot' %>');
    src: url('<%= asset_path 'RobotoSlab-Bold-webfont.eot?#iefix'%>') format('embedded-opentype'),
         url('<%= asset_path 'RobotoSlab-Bold-webfont.woff' %>') format('woff'),
         url('<%= asset_path 'RobotoSlab-Bold-webfont.ttf' %>') format('truetype'),
         url('<%= asset_path 'RobotoSlab-Bold-webfont.svg#roboto_slabbold' %>') format('svg');
    font-weight: normal;
    font-style: normal;

}

.legislators .mix{
    display: none;
}



