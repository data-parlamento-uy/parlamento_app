body {
  background: white;
  color: #666666;
  font-weight: 300;
}
p{
    font-weight: 300;
}
.cabezal{
    background-color: #ffffff;
    padding: 20px;
}
.top-bar{
    padding: 20px;
    background-color: #fff;
}

.title-area img{
     width: 278px;
    height: 102px;
    margin-left: 5em;
}
.top-bar-section li:not(.has-form) a:not(.button),
.top-bar-section li:not(.has-form) a:not(.button):hover{
    color: #9d9d9c;
    background: #fff;
}

.top-bar-section li.active:not(.has-form) a:not(.button),
.top-bar-section li.active:not(.has-form) a:not(.button):hover {
    color: #e86d72;
    background: white;
}
.menu-centrado{
    margin-top: 30px;
}
.menu{
    font-weight: 300;
}
.boton{
    color:#9d9d9c;
}
.boton.active, .boton:hover {
    color:#e86d72;
}
.margen-derecha{
    margin-right: 40px;
}
.margen-izquierda{
    margin-left: 40px;
}
.margen-superior{
    margin-top: 100px;
}
.margen-inferior{
    margin-bottom: 30px;
}

.margen-izquierda-mitad{
    margin-left: 10px;
}
.menu li:last-child{
    margin-right: 0 !important;
}
.menu input{
    font-size: 1em;
    font-weight: 300;
    border: none;
    box-shadow: none;
    display: inline;
    width: 150px;
    margin-left: 5px;
    margin-bottom: 0;
}
.menu  input[type="text"]:focus {
    box-shadow: none;
}
.menu img{
    width: 18px;
    height: 18px;
}
.top-bar input{
    font-size: 1em;
    font-weight: 300;
    border: none;
    box-shadow: none;
    position: relative;
    top: -36px;
    left: 20px;
}
.top-bar  input[type="text"]:focus {
    box-shadow: none;
}
.menu ul li{
    float: left;
    list-style-type: none;
    margin-left: 30px;
    padding: 20px;
}
.camara{
    margin-top: -40px;
    margin-bottom: -10px;
}
.camara ul li{
    float: left;
    list-style-type: none;
    padding: 0px;
}

.header{
    height: 700px;
    background-image: url('<%= asset_path 'manos.jpg' %>');
    background-position: bottom;
    background-size: 100%;
    background-repeat: no-repeat;
    padding: 10% 15% 0;
}
.header h1{
    margin-top: 30px;
    font-family: 'roboto_slabbold';
}
.header p{
    font-size:1.2em;
}
.representacion{
    height: 740px;
    padding: 1% 0;
    background: #f7f7f8;
}
.representacion h1{
    line-height: 0.6;
    margin-top: 30px;
    font-family: 'roboto_slabbold';
}
.representacion p{
    line-height: 1;
}
.representacion img{
    margin-top: 1.5em;
}
.subheader {
    font-size: 2em;
    line-height: 0;
    color: #222222;
    font-weight: normal;
    margin-top: 0em;
    margin-bottom: 3rem;
}
.asistencia-referencia{
    font-size: 1.8em;
    font-weight: 100;
    margin-top: -0.6em;
    margin-bottom: 0;
}
.valor-referencia{
    font-size: 2.75rem;
    color:#222222;
    font-family: 'roboto_slabthin';
    margin-top: -0.2em;
}
#porcentaje>.valor-referencia{
    font-size: 2.75rem;
    line-height: 1;
    color:#222222;
    font-family: 'roboto_slabthin';
}
.representacion hr{
    margin: 1.3rem 0 1.4875rem;
}

.cargo-referencia{
    margin-top: -1.2em;
    margin-bottom: -3em !important;
}

.fichas{
    background: #f7f7f8;
    padding-top: 20px;
}
.fichas h4{
    font-family: 'roboto_slabregular';
}
.fichas h6{
    font-family: 'roboto_slabregular';
}
.ficha{
    background: #fff;
    margin-bottom: 2em;
    border-radius: 6px;
}
.bandera{
    width: 86px;
    height: 86px;
}

.titulo-fichas{
    margin-top: -4em !important;
}
.deptos{
    font-size: 0.8em;
    line-height: 0.8em;
}
.fichas .valor-referencia{
    font-size: 2rem;
    color:#222222;
    font-family: 'roboto_slabregular';
    margin-top: 0;
}
.fichas .cargo-referencia{
    margin-top: -1.8em;
}

.temas{
    padding-top: 40px;
}

.temas h3{
    font-family: 'roboto_slabregular';
}

.temas h4{
    margin-top: 40px;
    font-family: 'roboto_slabregular';
}
.temas ul{
    list-style: none;
    margin-left: 0;
}
.temas li{
    color:#e86d72;
}
.temas .button{
    background: none;
    color: #e86d72;
    font-family: 'roboto_slablight';
}
.footer{
    padding:3.5em;
    height: 150px;
    background: #74c5cd;
    color: white;
}
.footer p{
    line-height: 0.5em;
    font-weight: 300;
    font-weight: 100;
    font-size: 1.3em;
    letter-spacing: 0.05em;
}
strong{
    font-weight: 600;
}

.parlamento{
    height: 800px !important;
    background-color: red;
}
.foto_perfil{
    border-radius: 100px;
}

.ficha-parlamentario{
    padding-top: 1em !important;
}
.datos-parlamentario{
    margin-top: -50px;
}
.bandera_derecha{
    width: 80px;
    height: 80px;
}
.nombre{
    line-height: 1;
}
.iconos{
    margin-top: -10px !important;
}
.iconos ul{
    margin-left: -0.9375rem;
}
.iconos li{
    float:left;
    list-style-type: none;
}
.iconos-expertise{
    width: 32px;
    height: 32px;
    margin-right: 6px;
}
.margen-derecho{
    padding-right: 0em !important;
}
.icono-correo{
    width: 22px;
    height: 27px;
    margin-right: 5px;
}
.icono-fb{
    width: 22px;
    height: 27px;
    margin-right: 5px;
}
.icono-tw{
    width: 22px;
    height: 27px;
    margin-right: 5px;
}
.dato-ficha{
    /*    margin-top: -1.8em;*/
    margin-bottom: 0;
}
.valor-parlamentario-arriba{
    font-size: 1.4rem;
    color:#222222;
    font-family: 'roboto_slabregular';
    margin-top: 30px;
    line-height: 1;
    margin-bottom: 0;
}
.valor-parlamentario-abajo{
    font-size: 1.4rem;
    color:#222222;
    font-family: 'roboto_slabregular';
    margin-top: 10px;
    line-height: 1;
    margin-bottom: 0;
}
.ultimo-dato{
    margin-bottom: 20px;
}
.filtro{
    margin-bottom: 0px;
}
.filtro ul{
    margin-bottom: 0.50em;
}
.label{
    background-color: #ffffff;
    color: #9d9d9c;
    font-weight: 300;
}
.label:hover, .label:active, .label.active {
    background-color: #e86d72;
    color: white;
}

.gusta{
    width: 36px;
    height: 33px;
    margin-bottom: 1.5em;
    background: url(<%= asset_path('gusta.png') %>) 0 0;
    background-size: 36px;
    background-repeat: no-repeat;
    cursor: pointer;
}
.gusta:hover, .gusta.active{
    width: 36px;
    height: 33px;
    margin-bottom: 1.5em;
    background: url(<%= asset_path('gusta.png') %>) 0 -33px;
    background-size: 36px;
    background-repeat: no-repeat;
    cursor: pointer;
}
.no-gusta{
    width: 36px;
    height: 33px;
    margin-bottom: 1.5em;
    background: url(<%= asset_path('no-gusta.png') %>)0 0;
    background-size: 36px;
    background-repeat: no-repeat;
    cursor: pointer;
}
.no-gusta:hover, .no-gusta.active{
    width: 36px;
    height: 33px;
    margin-bottom: 1.5em;
    background: url(<%= asset_path('no-gusta.png') %>) 0 -39px;
    background-size: 36px;
    background-repeat: no-repeat;
    cursor: pointer;
}

.comisiones a{
    color: #666666;
}

.comisiones a:hover, a.active {
  color: #e86d72;
}

.no_data {
    padding: 10px;
    text-align: center;
    margin-bottom: 0;
}

@media only screen and (min-width:1025px){
.row {
      max-width: 100%; }
}


/* #### FUENTES ####*/

@font-face {
    font-family: 'roboto_slabthin';
    src: url('<%= asset_path 'RobotoSlab-Thin-webfont.eot' %>');
    src: url('<%= asset_path 'RobotoSlab-Thin-webfont.eot?#iefix' %>') format('embedded-opentype'),
         url('<%= asset_path 'RobotoSlab-Thin-webfont.woff' %>') format('woff'),
         url('<%= asset_path 'RobotoSlab-Thin-webfont.ttf' %>') format('truetype'),
         url('<%= asset_path'RobotoSlab-Thin-webfont.svg#roboto_slabthin' %>') format('svg');
    font-weight: normal;
    font-style: normal;

}
